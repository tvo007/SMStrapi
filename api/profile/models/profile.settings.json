{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "name": "Profile"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "bio": {
      "type": "richtext"
    },
    "status": {
      "type": "string"
    },
    "userName": {
      "type": "string",
      "required": true
    }
  }
}
